<script>
    $(document).ready(function () {

        // 検索ダイアログオープン時に「カテゴリ内検索」のラジオを有効化するか否か
        $('#searchModal').on('show.bs.modal', function () {
            if (location.pathname.indexOf("/category") === 0) {
                $('#modalSearchType').show();
            }
        });

        // カテゴリ内、全体のradioボタンの挙動制御
        $('#modalRadioFromAll').change(function () {
            $(this).prop("checked", true);
            $('#modalRadioFromCategory').prop("checked", false);
            $('#modalSearchForm').attr('action', '/search');
        });
        $('#modalRadioFromCategory').change(function () {
            $(this).prop("checked", true);
            $('#modalRadioFromAll').prop("checked", false);
            $('#modalSearchForm').attr('action', location.pathname);
        });

        // TAG: 検索
        $('#tag-search').click(function (e){
            $('#search-input-q').val("TAG:"+ $(this).val() + " ");
            $('#searchModal').modal();
            $('#search-input-q').focus();
        });

        // beacon
        function beacon(href){
            $.ajax({
                url: '/bc/jump',
                type: 'POST',
                data: {
                    "csrf": $('meta[name="csrf-token"]').attr('content'),
                    "href": href,
                },
                dataType: "json"
            });
        }

        $("a[href^='https://twitter.com']").on('click', function (e){
            let href = $(this).attr('href');
            beacon(href);
        });
    });
</script>

<script id="categoryMenuJs" data-category-api-url="/api/v1/category?q=CATEGORY" data-category-detail-url="/category/%5BCATEGORY%5D">

    $(document).ready(function () {

        function isContract() {
            if ($(window).width() > 575.98) {
                return false;
            }
            let index = document.cookie.indexOf('contractScreen=1');
            return (index > -1);
        }

        function setSearchAction(type) {
            if (type == "category") {
                let href = $('#category-menu .selected > a').attr('href') ;
                $("#mainSearchBox").attr("action", href);
            } else {
                $("#mainSearchBox").attr("action", "/search");
            }
        }

        // 初期表示時にラジオ有効化
        $('#radioFromCategory').prop("disabled", false);

        // サイト内、カテゴリ選択でformのactionを変更
        $('input[name="type"]').change(function(){
            let type = $(this).val();
            setSearchAction(type);
        });

        $('.selectable-category').click(function(e){

            let categoryName = $(this).data("categoryName");
            let encadedCategoryName = encodeURIComponent(categoryName);

            // デザイン
            let parent = $(this).parent();
            let bgColor = parent.css("background-color");
            parent.parent().children().removeClass('selected');
            parent.addClass('selected').css("border-color", bgColor);

            // searchBoxのアクションを変更する。
            let type = $('input[name="type"]:checked').val();
            setSearchAction(type)

            // 中央寄せ
            let menu = parent.parent().get(0);
            // 全体幅
            let menu_real_width = menu.scrollWidth;
            // 表示幅
            let menu_display_width = parent.parent().width();
            // 要素幅
            let element_width = parent.width();
            // メニュー内の位置
            let position = parent.position();
            let position_left = parent.parent().scrollLeft() + position.left;
            // 調整幅
            let adjustment = position_left - menu_display_width / 2 + element_width / 2;
            if (menu_real_width < menu_display_width + adjustment) {
                adjustment =  menu_real_width - menu_display_width;
            }
            if (adjustment < 0 ){
                adjustment = 0;
            }
            parent.parent().scrollLeft(adjustment);

            // Ajax
            let api_url = $('#categoryMenuJs').data('categoryApiUrl');
            api_url = api_url.replace('CATEGORY', encadedCategoryName);
            $.ajax({
                type: 'GET',
                url: api_url, // 呼び出すURL
                dataType: 'html',
                success: function (data) {
                    $('#tweets-area').empty().html(data);

                    // TODO:
                    let lazyImages = [].slice.call(document.querySelectorAll("img.lazy"));
                    lazyImages.forEach(function (lazyImage) {
                        lazyImage.src = lazyImage.dataset.src;
                    });

                    // ツイートサイズの再計算
                    if (isContract()){
                        $("#navbarContractIcon").trigger('click');
                    }
                }
            });
            let base = $('#categoryMenuJs').data('categoryDetailUrl');

            $('#moreViewButton').attr('title', categoryName+'のイラストをもっと見る').attr('href', base.replace('[CATEGORY]', encadedCategoryName + "?offset=20"));
            return false;
        });

    });
</script>
</body>
</html>
