                oRange = window.getSelection().getRangeAt(0);
                oRange.getClientRects();
                position = oRange.getClientRects()[0]
            } else if (document.selection) {
                selectedText = document.selection.createRange().text;
                oRange = window.getSelection().getRangeAt(0);
                position = oRange.getBoundingClientRect();
            }
            if (selectedText === "") {
                return;
            }
            if (selectedText.indexOf("\n") > -1) {
                return;
            } else if (selectedText.indexOf("\r") > -1) {
                return;
            }

            // 座標計算
            let offset = $(this).parent().offset();
            offset = $(window).scrollTop();
            searchSupportBox.css({
                "top": offset + position.y +  position.height + 8,
                "left":  position.x
            });

            // リンク先URL
            let query = {
                "q" : selectedText
            };
            let href = "/search?" +  $.param(query);

            if (searchSupportText.length === 0) {
                searchSupportText = $("<a data-google-interstitial=false></a>").attr('id', "serchSupportText").html(
                    '<svg width="14" height="14" style="color:white;"><use xmlns:xlink="http://www.w3.org/1999/xlink" xlink:href="#search" /></svg>Buhitter! 検索'
                );
                searchSupportText.mousedown(function(e){
                            e.stopPropagation()
                });
                searchSupportBox.append(searchSupportText);
            }
            searchSupportText.attr('href', href);

            // 表示
            searchSupportBox.show("fast");
        });

        // // 検索リンクをクリックした時にリンクボックス閉じさせないようにする。
        // $("#serchSupportText").on('mousedown', function(e){
        //     e.stopPropagation()
        // });
    });
</script>


